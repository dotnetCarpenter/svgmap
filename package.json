  "dependencies": {
    "browserify": "^12.0.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "live-server": "git+https://github.com/dotnetCarpenter/live-server.git",
    "tape": "^4.2.2"
  }
}
